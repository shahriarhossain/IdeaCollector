{{#each ideas}}
    <div class="container">
        <div class="card card-box mb-3">
            <H1>{{title}}</H1>
            <p>{{description}}</p>
            <a class="btn btn-dark mb-2" href="/Ideas/Edit/{{id}}"><i class="far fa-edit"></i>Edit</a>
            <a class="btn btn-danger mb-2" href="/Ideas/Delete/{{id}}">Delete</a>
        </div>
    </div>
{{else}}
    <p>No Data to show!</p>
{{/each}}
